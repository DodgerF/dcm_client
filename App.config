<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Client.Properties.Settings"
               type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
               requirePermission="false" />
    </sectionGroup>
  </configSections>

  <appSettings>
    <!-- Базовый URL для HTTP-запросов к серверу -->
    <add key="ServerBaseUrl" value="https://localhost:8444/" />
    <!-- Другие настройки при необходимости -->
    <!-- <add key="SomeOtherKey" value="SomeValue" /> -->
  </appSettings>

  <!-- Если вы используете механизм Settings.settings -->
  <applicationSettings>
    <Client.Properties.Settings>
      <setting name="ServerBaseUrl" serializeAs="String">
        <value>https://localhost:8444/</value>
      </setting>
      <!-- Можно добавить здесь и другие параметры из Settings.settings -->
    </Client.Properties.Settings>
  </applicationSettings>

  <!-- Параметры старта приложения -->
  <startup>
    <!-- Укажите ту версию .NET Framework, на которой вы собираете клиент -->
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>
